        Url =
            function () { }
        Url.prototype = {
            _relativeRoot: '@Url.Content("~")',
            // create an extension method called "resolve"
            resolve: function (relative) {
                var resolved = relative;
                if (relative.charAt(0) == '~')
                    resolved = this._relativeRoot + relative.substring(2);
                return resolved;
            }
        }
        $Url = new Url();
    </script>
    <script>

        function clickCheckBox(e) {
            var radio = $(e);
            if (radio.data("waschecked") == true) {
                radio.prop("checked", false);
                radio.data("waschecked", false);
            } else {
                radio.data("waschecked", true);
            }

        }
        function changeRange(e) {
            if (questions && questions.length == 8)
                return;

            var idRange = '#' + $(e).attr('id');

            if (idRange) {
                // show feedback textarea if the rating is 2 or less
                if ($(e).val() <= 2) {
                    $('#msQfeed_' + idRange.substring(9)).css('display', 'block');
                }

                // hide the feedback textarea if the rating is 3 and above
                if ($(e).val() >= 3) {
                    $('#msQfeed_' + idRange.substring(9)).css('display', 'none')
                    $('#msQfeed_' + idRange.substring(9)).val('');
                }
            }
        }
    </script>

